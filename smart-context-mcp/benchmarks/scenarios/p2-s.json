{
  "name": "p2-s",
  "description": "Profile S: Small (~50k embeddings, ~10k files) - Basic development/local environment",
  "includeGlobs": ["src/**"],
  "excludeGlobs": ["benchmarks/**", "dist/**", "node_modules/**"],
  "queries": [
    { "query": "class SearchEngine", "expectTop": "src/engine/Search.ts" },
    { "query": "SymbolIndex", "expectTop": "src/ast/SymbolIndex.ts" },
    { "query": "parseIntent", "expectTop": "src/orchestration/IntentRouter.ts" }
  ],
  "vectorIndex": {
    "mode": "auto",
    "shards": "off"
  },
  "embeddingPack": {
    "format": "float32"
  }
}
